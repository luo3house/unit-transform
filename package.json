{
  "name": "unit-transform",
  "version": "0.0.1",
  "description": "Celsius 2 Fahrenheit unit transformer",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "license": "MIT",
  "author": "luoyz <luoyz2349@outlook.com>",
  "private": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build:types": "tsc",
    "build:cjs": "babel --config-file ./.babelrc.cjs.json --extensions '.ts' --source-maps -d dist/cjs/ src/",
    "build:esm": "babel --config-file ./.babelrc.esm.json --extensions '.ts' --source-maps -d dist/esm/ src/",
    "build:umd": "rollup -c rollup.config.ts",
    "build": "rimraf -rf ./dist && yarn build:cjs && yarn build:esm && yarn build:umd && yarn build:types",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/jest": "^28.1.7",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.78.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4"
  }
}
